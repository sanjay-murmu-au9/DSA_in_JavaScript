           function addUpTo(n)
 {
     let total = 0;
     for (let i = 1; i <= n; i++)
     {
         total += i;
     }
     return total;
 }

 let t1 = performance.now();
 addUpTo(10000000)
     let t2 = performance.now()
                  console.log(`Time Elapsed
                              : ${(t2 - t1) / 1000} seconds.`) // to covert from minisec to sec;

