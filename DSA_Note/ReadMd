# 25 most asked interview question and answers
function pallindrome(str){
  let len = str.length;

  for(let i=0;i<len/2;i++){
    if(str[i] !== str[len - 1 - i]){
      return console.log('its not pallindrome!!')
    }
  }
  console.log('Its pallindrome!!')
}


pallindrome('levEl');


